    <servlet>
        <servlet-name>Provider_Login_Action</servlet-name>
        <servlet-class>provider.Provider_Login_Action</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Broker_Login_Action</servlet-name>
        <servlet-class>broker.Broker_Login_Action</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Accept_Provider</servlet-name>
        <servlet-class>provider.Accept_Provider</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>provider.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User_Register_Action</servlet-name>
        <servlet-class>user.User_Register_Action</servlet-class>
