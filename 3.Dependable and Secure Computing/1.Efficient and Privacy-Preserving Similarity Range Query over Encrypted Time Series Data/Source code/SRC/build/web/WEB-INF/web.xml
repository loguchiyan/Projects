        <url-pattern>/Accept_Provider</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_Register_Action</servlet-name>
        <url-pattern>/User_Register_Action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_Login_Action</servlet-name>
        <url-pattern>/User_Login_Action</url-pattern>
