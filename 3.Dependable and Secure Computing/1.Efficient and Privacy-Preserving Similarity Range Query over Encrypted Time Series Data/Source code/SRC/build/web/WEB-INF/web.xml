    </servlet>
    <servlet>
        <servlet-name>Proxy_Request</servlet-name>
        <servlet-class>broker.Proxy_Request</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Proxy_Login_Action</servlet-name>
        <servlet-class>broker.Proxy_Login_Action</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Permit_Action</servlet-name>
        <servlet-class>broker.Permit_Action</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>okgsp_Login_Action</servlet-name>
        <servlet-class>broker.okgsp_Login_Action</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Provider_Register_Action</servlet-name>
        <url-pattern>/Provider_Register_Action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Provider_Login_Action</servlet-name>
        <url-pattern>/Provider_Login_Action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Broker_Login_Action</servlet-name>
        <url-pattern>/Broker_Login_Action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
