        <url-pattern>/Permit_Action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>okgsp_Login_Action</servlet-name>
        <url-pattern>/okgsp_Login_Action</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
